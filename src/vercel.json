{
  "version": 2,
  "functions": {
    "src/api/bg_remove.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/bg_remove",
      "dest": "/src/api/bg_remove.js"
    },
    {
      "src": "/index.js",
      "dest": "/src/public/index.js"
    },
    {
      "src": "/css/style.css",
      "dest": "/src/public/style.css"
    },
    {
      "src": "/(.*)",
      "dest": "/src/public/index.html"
    }
  ]
}
